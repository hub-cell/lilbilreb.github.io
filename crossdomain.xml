<allow-http-request-headers-from domain="st.mycdn.me" headers="*"/>
<allow-http-request-headers-from domain="stest.mycdn.me" headers="*"/>
<allow-http-request-headers-from domain="ok.ru" headers="*"/>
<allow-http-request-headers-from domain="*.facebook.com" headers="*"/>
