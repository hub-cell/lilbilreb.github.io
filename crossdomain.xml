<allow-access-from domain="ok.ru"/>
<allow-access-from domain="*.ok.ru"/>
<allow-access-from domain="ok.me"/>
<allow-access-from domain="*.facebook.com"/>
<allow-access-from domain="facebook.com"/>
<allow-access-from domain="*"/>
</cross-domain-policy>
